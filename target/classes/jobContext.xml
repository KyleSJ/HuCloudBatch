<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean	id="printNowDateDao"	
			class="kr.co.hucloud.batch.job.print.now.date.dao.impl.PrintNowDateDAOImpl">
		<property name="sqlSessionTemplate"	ref="sqlSessionTemplate" />		
	</bean>

	<bean	id="printNowDateBiz"
			class="kr.co.hucloud.batch.job.print.now.date.biz.impl.PrintNowDateBizImpl">
		<property name="printNowDAO"	ref="printNowDateDao" />
	</bean>
	
	<bean	id="matchingService"
			class="kr.co.hucloud.batch.job.matching.service.MatchingServiceImpl">
		<property	name="matchingDao"	ref="matchingDao" />
	</bean>
	
	<bean	id="matchingDao"
			class="kr.co.hucloud.batch.job.matching.dao.MatchingDaoImpl">
		<property	name="sqlSessionTemplate" ref="sqlSessionTemplate" />		
	</bean>
	
</beans>
